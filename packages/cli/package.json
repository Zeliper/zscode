{
  "name": "@zeliper/zscode",
  "version": "1.0.3",
  "description": "ZSCode CLI - Initialize and manage ZSCode Planning System in your projects",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "zscode": "./dist/index.js"
